{
  "name": "JOHARFIX",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "bundle:android": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android.bundle --sourcemap-output android.bundle.map --assets-dest .",
    "analyze:android": "npm run bundle:android && npx react-native-bundle-visualizer android.bundle android.bundle.map",
    "analyze:ios": "npm run bundle:ios && npx react-native-bundle-visualizer ios.bundle",
    "bundle:ios": "react-native bundle --platform ios --dev false --entry-file index.js --bundle-output ios.bundle --assets-dest ."
  },
  "dependencies": {
    "@react-native/new-app-screen": "0.82.1",
